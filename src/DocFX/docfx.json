{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Fusee/Fusee.sln"
          ],
        }
      ],
      "dest": "api",
	  "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },      {
        "files": [
          "examples/rocket/**",
	      "examples/picking/**"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
	  {
        "files": [
          "wiki/**.md",
          "wiki/toc.yml"
        ]
      },
	  	  {
        "files": [
          "getting-started/**.md",
          "getting-started/toc.yml"
        ]
      },
	  	  	  {
        "files": [
          "examples/**.md",
          "examples/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "getting-started/images/**"
        ]
      },
	  {
        "files": [
          "examples/examples/**"
        ]
      },
	  {
        "files": [
          "examples/rocket/**"
        ]
      },
	  {
        "files": [
          "examples/picking/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
	  "templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}